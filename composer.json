{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "ext-mongodb": "^1.14",
        "fideloper/proxy": "^4.4",
        "guzzlehttp/guzzle": "^7.2",
        "jenssegers/mongodb": "^3.9",
        "laravel/framework": "^9.19",
        "laravel/passport": "^11.3",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "mll-lab/laravel-graphql-playground": "^2.6",
        "php-open-source-saver/jwt-auth": "^2.0",
        "rebing/graphql-laravel": "^8.3",
        "webpatser/laravel-uuid": "^4.0",
        "yucadoo/laravel-geojson-rule": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "exclude-from-classmap": ["vendor\\laravel\\sanctum\\src\\PersonalAccessToken.php"],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Laravel\\Sanctum\\": "app/Models/Sanctum/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "onboarding": [ 
            "composer install",
            "composer clear",
            "composer dump-autoload",
            "php artisan migrate:fresh",
            "php artisan db:seed",
            "php artisan key:generate",
            "php artisan optimize",
            "php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
